{
    "name": "advaith/seamless-admin",
    "description": "A seamless django like admin panel setup for Laravel",
    "type": "library",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Advaith\\SeamlessAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Advaith\\SeamlessAdmin\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Advaith A J",
            "email": "advaitharunjeena@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Advaith\\SeamlessAdmin\\SeamlessAdminServiceProvider"
            ],
            "aliases": {
                "SeamlessAdmin": "Advaith\\SeamlessAdmin\\SeamlessAdmin"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^5.0|^6.23|^7.0|^8.0|^9.0",
        "laravel/laravel": "^7.0|^8.0|^9.0|^10.0"
    }
}
