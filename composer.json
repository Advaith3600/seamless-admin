{
    "name": "advaith/seamless-admin",
    "description": "A seamless django like admin panel setup for Laravel",
    "type": "library",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^11.0",
        "orchestra/testbench": "^9.0",
        "laravel/laravel": "^11.0",
        "nesbot/carbon": ">=2.62.1",
        "symfony/console": ">=4.3.4",
        "mockery/mockery": "^1.3.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Advaith\\SeamlessAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Advaith\\SeamlessAdmin\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Advaith A J",
            "email": "advaitharunjeena@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Advaith\\SeamlessAdmin\\SeamlessAdminServiceProvider"
            ],
            "aliases": {
                "SeamlessAdmin": "Advaith\\SeamlessAdmin\\SeamlessAdmin"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.1"
    }
}
