{
  "name": "seamless-admin",
  "version": "1.0.0",
  "description": "A seamless django like admin panel setup for Laravel",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "keywords": [
    "laravel",
    "admin"
  ],
  "author": "Advaith A J",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "laravel-mix": "^6.0.43",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "vue": "^3.2.31",
    "vue-loader": "^16.2.0"
  }
}
